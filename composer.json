{
    "name": "nexium-mini/foundation",
    "description": "This is a mini from illuminate/foundation.",
    "keywords": ["framework", "nexium"],
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "files": [
            "src/Illuminate/Foundation/helpers.php"
        ],
        "psr-4": {
            "Illuminate\\": "src/Illuminate/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.5-dev"
        }
    },
    "scripts": {
        "sync": "@php bin/sync"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
